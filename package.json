{
  "name": "TTKT",
  "version": "1.0.0",
  "description": "TTKT",
  "keywords": [
    "TypeScript",
    "express",
    "nodejs",
    "boilerplate",
    "eslint"
  ],
  "author": "TTKT",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "build": "tsc && rm -r dist/public && cp -r src/public/ dist/public/",
    "lint": "eslint src --ext .ts",
    "dev": "concurrently \"tsc-watch\" \"nodemon dist/app.js\"",
    "dev-srvr": "tsc-watch",
    "start": "concurrently \"tsc\" \"node dist/app.js\"",
    "dev-sls": "concurrently \"tsc-watch\" \"sls offline -s dev -r ap-southeast-1 --useChildProcesses\"",
    "test": "yarn build && echo 'tests failing'",
    "fix-lint": "eslint src --ext .ts --fix",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "amazon-cognito-identity-js": "^4.5.4-unstable.1",
    "aws-sdk": "^2.785.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "husky": "^4.3.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "lodash": "^4.17.20",
    "mongoose": "^5.12.5",
    "pug": "^3.0.0",
    "pusher": "^4.0.2",
    "serverless-http": "^2.6.0",
    "swagger-ui-express": "4.1.4",
    "tsc-watch": "^4.2.9",
    "uuid": "^8.3.2",
    "winston": "3.3.3",
    "winston-loggly-bulk": "^3.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/bcrypt": "^3.0.1",
    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/hapi__joi": "17.1.2",
    "@types/jest": "26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/lodash": "^4.14.167",
    "@types/mongoose": "*",
    "@types/node": "14.0.13",
    "@types/node-fetch": "^2.5.8",
    "@types/pug": "^2.0.4",
    "@types/supertest": "2.0.9",
    "@types/swagger-ui-express": "4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/winston-loggly-bulk": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "concurrently": "^5.3.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.21.2",
    "mockingoose": "2.13.2",
    "nodemon": "^2.0.7",
    "serverless-domain-manager": "^5.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-prune-plugin": "^1.4.3",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "typescript": "^4.0.5"
  },
